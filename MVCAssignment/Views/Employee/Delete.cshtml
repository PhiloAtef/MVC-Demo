@model Employee
@{
    ViewData["Title"] = "Delete";
}

<h1> Are you sure you want to delete this Employee?</h1>
<br />
<br />

<form asp-action="Delete" asp-route-id="@Model.Id" >
    
    <partial name="PartialViews/DetailsDeleteEmployeePartial"></partial>
    <input type="submit" name="Delete" class="btn btn-danger" value="" />
    <a asp-action="Index" class="btn btn-light mt-5">Cancel</a>


</form>

